//Fenotipos (A, B, AB, O)

Program HelloWorld;
var
  a: integer;
  b: integer;
  ab: integer;
  o: integer;
begin
  write('Digite o valor de A ');
  readln(a); 

  write('Digite o valor de B ');
  readln(b); 

  write('Digite o valor de AB ');
  readln(ab); 

  write('Digite o valor de O ');
  readln(o); 

  writeln('A => ', a);  
  writeln('B => ', b);  
  writeln('AB => ', ab);  
  writeln('O => ', o);  
end.





